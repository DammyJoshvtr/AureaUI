{
  "expo": {
    "name": "Aurea Kit",
    "slug": "Aurea Kit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/logo.svg",
    "scheme": "client",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/logo/logo.svg",
        "backgroundImage": "./assets/logo/logo.svg",
        "monochromeImage": "./assets/logo/logo.svg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.AureaKit"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/logo/logo.svg",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo/logo.svg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
